class ListTestClass extends EntryPoint is
    method assert(cond : Boolean) : ListTestClass is
        if cond.Not() then
            this.ExitCode := 1
        end
        return this
    end

    this() is
        var list: List[Integer]()
        list := list.append(1)
        list := list.append(20)
        list := list.append(100)

        this := this.assert(list.get(0).Equal(1)) // 1
        this := this.assert(list.get(1).Equal(20)) // 20
        this := this.assert(list.head().Equal(1)) // 1
        this := this.assert(list.tail().Equal(100)) // 100
    end
end