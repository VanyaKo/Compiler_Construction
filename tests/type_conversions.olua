class TestTypeConversion is
    method assert(cond : Boolean) then
        if cond.Not() then
            this.ExitCode = 1
        end
    end

    this(stdin: CharInput, stdout: CharOutput, stderr: CharOutput, args: Array[Array[Integer]]) is
        var a : Integer(5)
        var b : Real(3.7)
        var resultInt : Integer
        var resultReal : Real
	
        resultReal := a.toReal // Expected: 5.0
        resultInt := b.toInteger // Expected: 3 (assuming truncation)
	assert(resultReal.Equal(5.0))
	assert(resultInt.Equal(3))
    end
end
