class Void extends EntryPoint is
    var a : Integer := 0

    method assert(cond : Boolean) : void is
        if cond.not() then
            this.exit_code := 1
        end
    end

    method doSmth() : void is
        this.a := this.a.plus(2)
    end

    this(stdin: StdIn, stdout: StdOut, stderr: StdOut, args: Array[Array[Integer]]) is
        this.assert(this.a.equal(0))
        this.doSmth()
        this.assert(this.a.equal(2))
    end
end
